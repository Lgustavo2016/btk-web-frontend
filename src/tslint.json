{
    "extends": "../tslint.json",
    "rules": {
        "directive-selector": [
            true,
            "attribute",
            "btk",
            "camelCase"
        ],
        "component-selector": [
            true,
            "element",
            "btk",
            "kebab-case"
        ],
        "prefer-const": false,
        "variable-name": false,
        "no-unused-expression": false
    }
}
